<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="es.tessier.carlos.misproyectos" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".GoogleJSON"
            android:label="@string/title_activity_google_json" >
        </activity>

        <activity
            android:name=".Actividad_Intent_1"
            android:label="@string/title_activity_actividad__intent_1" >
        </activity>

        <activity
            android:name=".Actividad_Intent_1_b"
            android:label="@string/title_activity_actividad__intent_1_b" >
        </activity>

        <activity
            android:name=".Actividad_Intent_2"
            android:label="@string/title_activity_actividad__intent_2" >
        </activity>

        <activity
            android:name=".Actividad_Intent_2_b"
            android:label="@string/title_activity_actividad__intent_2_b" >
        </activity>

        <activity
            android:name=".Actividad_Intent_3"
            android:label="@string/title_activity_actividad__intent_3" >
        </activity>

        <activity
            android:name=".Actividad_Intent_3_b"
            android:label="@string/title_activity_actividad__intent_3_b" >
        </activity>

        <activity
            android:name=".Actividad_Ciclo_De_Vida"
            android:label="@string/title_activity_ciclo_de_vida" >
        </activity>

        <activity
            android:name=".Actividad_Pulsame"
            android:label="@string/title_activity_pulsame" >
        </activity>

        <activity
            android:name=".Actividad_Implicit_Intents"
            android:label="@string/title_activity_implicit_intents" >
        </activity>


        <receiver
            android:name=".Broadcast_Receiver"
            android:enabled="true"
            android:exported="true" >
            <intent-filter>
                <action android:name="es.tessier.carlos.misproyectos.CUSTOM_INTENT" />
            </intent-filter>
        </receiver>

        <activity
            android:name=".Actividad_Broadcast_Receiver"
            android:label="@string/title_activity_actividad__broadcast__receiver" >
            <intent-filter>
                <action android:name="es.tessier.carlos.misproyectos.CUSTOM_INTENT" />
            </intent-filter>
        </activity>




    </application>

</manifest>
